  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "start:en": "ng serve --configuration=en",
    "start:pt": "ng serve --configuration=pt",
    "start:es": "ng serve --configuration=es",
    "build": "ng build",
    "build-prod": "node --max_old_space_size=8192 node_modules/@angular/cli/bin/ng build --prod",
    "build-sandbox": "node --max_old_space_size=8192 node_modules/@angular/cli/bin/ng build --configuration=sandbox",
    "build:en": "ng build  --configuration=en",
    "build:pt": "ng build  --configuration=pt",
    "build:es": "ng build  --configuration=es",
    "build-i18n:en": "ng build --output-path=dist/en --aot -prod --bh /en/ --i18n-file=src/i18n/messages.en.xlf --i18n-format=xlf --locale=en",
    "build-i18n:pt": "ng build --output-path=dist/pt --aot -prod --bh /pt/ --i18n-file=src/i18n/messages.pt.xlf --i18n-format=xlf --locale=pt",
    "build-i18n:es": "ng build --output-path=dist/es --aot -prod --bh /es/ --i18n-file=src/i18n/messages.es.xlf --i18n-format=xlf --locale=es",
    "test": "ng test",
    "lint": "ng lint",
    "e2e": "ng e2e",
    "int:extract": "ng xi18n --output-path locale"
  },
